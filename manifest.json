{
   "name": "BugMeNot",
   "description": "Give you a list of users for te current page.",
   "version": "27.0",
   "manifest_version": 2,

   "browser_action": {
      "default_icon": "assets/img/icon_16.png",
      "default_popup": "popup.html"
   },
   "background": {
      "scripts": ["assets/libs/jquery-3.3.1.min.js","assets/js/background.js"],
      "persistent": true
   },
   "icons": {
      "128": "assets/img/icon_128.png",
      "16": "assets/img/icon_16.png",
      "48": "assets/img/icon_48.png"
   },
   "permissions": [
      "http://bugmenot.com/*",
      "https://cdnjs.cloudflare.com/*",
      "http://cdnjs.cloudflare.com/*",
      "tabs",
      "storage"
   ]
}
